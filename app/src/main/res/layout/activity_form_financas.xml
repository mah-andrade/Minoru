<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    xmlns:android="http://schemas.android.com/apk/res/android" >

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:background="@color/fundo"
    android:layout_height="match_parent"
    tools:context=".forms.telagerentel_layout.form_financas">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:text="Finanças"
        app:layout_constraintStart_toStartOf="@id/containerComponents"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@id/containerComponents"
        app:layout_constraintVertical_bias="0.992" />




    <ImageView
        android:id="@+id/logominoru"
        android:layout_width="180dp"
        android:layout_height="100dp"
        android:layout_marginTop="90dp"
        android:src="@drawable/minorulogo3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ajuda"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/ajuda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/logominoru"
        app:layout_constraintTop_toTopOf="parent" />



    <ImageView
        android:id="@+id/voltar"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_volt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintStart_toEndOf="@id/logominoru"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/containerComponents"
        style="@style/ContainerComponentsp"
        android:layout_height="600dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logominoru"
        app:layout_constraintVertical_bias="1.0" />


    <View
        android:id="@+id/containerComponents5"
        style="@style/ContainerComponents3"
        android:layout_width="390dp"
        android:layout_height="55dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/containerComponents" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginStart="30dp"
        android:text="selecione um mês"
        android:textColor="@color/black"
        android:textSize="23dp"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/containerComponents5" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="5dp"
        android:text="Relatório do mês"
        android:textColor="@color/azul"
        android:textStyle="bold"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5" />


    <TextView
        android:id="@+id/txtttcliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_marginStart="15dp"
        android:text="Total de clientes"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5" />


    <TextView
        android:id="@+id/nclientes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="0"
        android:textColor="@color/red2"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/txtttcliente" />

    <TextView
        android:id="@+id/txtttmensal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Total de mensalistas"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@id/txtttcliente"
        app:layout_constraintTop_toBottomOf="@id/txtttcliente" />

    <TextView
        android:id="@+id/nmensalistas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="0"
        android:textColor="@color/red2"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/txtttmensal" />


    <TextView
        android:id="@+id/txtsalarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Salarios"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@id/txtttmensal"
        app:layout_constraintTop_toBottomOf="@id/txtttmensal" />


    <TextView
        android:id="@+id/vsalarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text=" R$ 0"
        android:textColor="@color/red2"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/txtsalarios" />

    <TextView
        android:id="@+id/txtredimentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Estacionamento"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@id/txtsalarios"
        app:layout_constraintTop_toBottomOf="@id/txtsalarios" />


    <TextView
        android:id="@+id/nredimentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="R$ 0"
        android:textColor="@color/red2"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/txtredimentos" />


    <TextView
        android:id="@+id/txtlucro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Saldo final"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@id/txtredimentos"
        app:layout_constraintTop_toBottomOf="@id/txtredimentos" />


    <TextView
        android:id="@+id/nlucro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="R$ 0"
        android:textColor="@color/red2"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@id/containerComponents5"
        app:layout_constraintTop_toBottomOf="@id/containerComponents5"
        app:layout_constraintTop_toTopOf="@id/txtlucro" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="10dp"
        android:spinnerMode="dropdown"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/containerComponents" />


</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>