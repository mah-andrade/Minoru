<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    xmlns:android="http://schemas.android.com/apk/res/android" >

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/fundo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".forms.telagerentel_layout.form_precos">



    <ImageView
        android:id="@+id/logominoru"
        android:layout_width="180dp"
        android:layout_height="100dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/minorulogo3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ajuda"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/ajuda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/logominoru"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:text="Preços"
        app:layout_constraintStart_toStartOf="@id/containerComponents"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@id/containerComponents"
        app:layout_constraintVertical_bias="0.992" />




    <ImageView
        android:id="@+id/voltar"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_volt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintStart_toEndOf="@id/logominoru"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/containerComponents"
        style="@style/ContainerComponentsp"
        android:layout_height="300dp"
        android:layout_width="400dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logominoru"
        app:layout_constraintVertical_bias="1.0" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:textStyle="bold"
        android:textSize="30dp"
        android:textColor="@color/black"
        android:text="Carro"
        app:layout_constraintStart_toStartOf="@id/containerComponents"
        app:layout_constraintTop_toTopOf="@+id/containerComponents"
        app:layout_constraintEnd_toEndOf="@id/containerComponents"
        app:layout_constraintVertical_bias="0.992" />


    <TextView
        android:id="@+id/horacarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:textSize="30dp"
        android:layout_marginStart="25dp"
        android:textColor="@color/black"
        android:text="Hora"
        app:layout_constraintStart_toStartOf="@id/containerComponents"
        app:layout_constraintTop_toTopOf="@+id/containerComponents"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/horavalorcarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginEnd="30dp"
        android:textColor="@color/dark_red"
        android:text="R$5,00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/horacarro" />


    <TextView
        android:id="@+id/diariocarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:text="Diário"
        app:layout_constraintStart_toStartOf="@id/horacarro"
        app:layout_constraintTop_toBottomOf="@id/horacarro"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/diarovalorcarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/dark_red"
        android:text="R$25,00"
        app:layout_constraintEnd_toEndOf="@id/horavalorcarro"
        app:layout_constraintTop_toTopOf="@+id/diariocarro" />


    <TextView
        android:id="@+id/mensalocarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:text="Mensal"
        app:layout_constraintStart_toStartOf="@id/horacarro"
        app:layout_constraintTop_toBottomOf="@id/diariocarro"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/mensalvalorcarro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/dark_red"
        android:text="R$300,00"
        app:layout_constraintTop_toBottomOf="@id/diarovalorcarro"
        app:layout_constraintEnd_toEndOf="@id/horavalorcarro"
        app:layout_constraintTop_toTopOf="@+id/mensalocarro" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_alt_carro"
        style="@style/Button"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="12dp"
        android:padding="1dp"
        android:text="Alterar"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@id/containerComponents"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/containerComponents3"
        style="@style/ContainerComponentsp"
        android:layout_height="300dp"
        android:layout_width="400dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerComponents"
        app:layout_constraintVertical_bias="1.0" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:textStyle="bold"
        android:textSize="30dp"
        android:textColor="@color/black"
        android:text="Moto"
        app:layout_constraintStart_toStartOf="@id/containerComponents3"
        app:layout_constraintTop_toTopOf="@+id/containerComponents3"
        app:layout_constraintEnd_toEndOf="@id/containerComponents3"
        app:layout_constraintVertical_bias="0.992" />


    <TextView
        android:id="@+id/horamoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:textSize="30dp"
        android:layout_marginStart="25dp"
        android:textColor="@color/black"
        android:text="Hora"
        app:layout_constraintStart_toStartOf="@id/containerComponents3"
        app:layout_constraintTop_toTopOf="@+id/containerComponents3"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/horavalormoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginEnd="30dp"
        android:textColor="@color/dark_red"
        android:text="R$3,00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/horamoto" />


    <TextView
        android:id="@+id/diariomoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:text="Diário"
        app:layout_constraintStart_toStartOf="@id/horamoto"
        app:layout_constraintTop_toBottomOf="@id/horamoto"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/diarovalormoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/dark_red"
        android:text="R$15,00"
        app:layout_constraintEnd_toEndOf="@id/horavalormoto"
        app:layout_constraintTop_toTopOf="@+id/diariomoto" />


    <TextView
        android:id="@+id/mensalomoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:text="Mensal"
        app:layout_constraintStart_toStartOf="@id/horamoto"
        app:layout_constraintTop_toBottomOf="@id/diariomoto"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/mensalvalormoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/dark_red"
        android:text="R$165,00"
        app:layout_constraintTop_toBottomOf="@id/diarovalormoto"
        app:layout_constraintEnd_toEndOf="@id/horavalormoto"
        app:layout_constraintTop_toTopOf="@+id/mensalomoto" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_alt_moto"
        style="@style/Button"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="12dp"
        android:padding="1dp"
        android:text="Alterar"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@id/containerComponents3"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>